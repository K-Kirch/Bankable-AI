{
  "name": "bankable-ai",
  "version": "0.1.0",
  "description": "Agentic Credit Intelligence Platform - AI-powered Bankability Score and remediation roadmap",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:unit": "vitest run --dir tests/unit",
